[commit]
	message = n
[merge]
	allow = true
[vrun]
	silent = false
	async = false
[deploy]
	host = kvm_nat
	path = /
[remote "github"]
    url = git@github.com:huangyingw/kvm_nat.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = github
[branch "dev"]
    remote = github
    merge = refs/heads/dev
